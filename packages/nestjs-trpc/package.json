  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.3.8 || ^10.0.0",
    "@nestjs/core": "^9.3.8 || ^10.0.0",
    "@trpc/server": "^10.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "rxjs": "7.8.1",
    "zod": "^3.14.0"
  },
  "devDependencies": {
    "@nestjs/common": "10.4.1",
