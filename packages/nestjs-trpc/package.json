    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "ts-jest": "^29.2.5",
    "ts-node": "10.9.2",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.21.0",
    "typescript": "5.5.3",
    "zod": "^3.14.4"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "ts-morph": "22.0.0",
    "tslib": "^2.5.0"
